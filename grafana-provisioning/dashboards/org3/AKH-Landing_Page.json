          },
          "query": "import \"date\"\n\ndp2 = from(bucket: \"AHK-AKDL\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\")\n  |> filter(fn: (r) => r[\"topic\"] == \"2024/BB/255\")\n  |> filter(fn: (r) => r[\"_field\"] == \"data_AN3\")\n  |> map(fn: (r) => ({\n       _time: r._time,\n       _value: ((float(v: r._value) - 4.0) / (20.0 - 4.0)) * 204.1/ 60.0 / 1000.0\n     }))\n  |> aggregateWindow(every: 1m, fn: sum)\n  |> sum(column: \"_value\")\n  |> map(fn: (r) => ({ _time: r._time, \"Daily Water Consumption (m³)\": r._value }))\n  |> keep(columns: [\"_time\", \"Daily Water Consumption (m³)\"])\n\n\ndp3 = from(bucket: \"AHK-AKDL\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\")\n  |> filter(fn: (r) => r[\"topic\"] == \"2024/BB/269\")\n  |> filter(fn: (r) => r[\"_field\"] == \"data_AN4\")\n  |> map(fn: (r) => ({\n       _time: r._time,\n       _value: ((float(v: r._value) - 4.0) / (20.0 - 4.0)) * 459.6/ 60.0 / 1000.0\n     }))\n  |> aggregateWindow(every: 1m, fn: sum)\n  |> sum(column: \"_value\")\n  |> map(fn: (r) => ({ _time: r._time, \"Daily Water Consumption (m³)\": r._value }))\n  |> keep(columns: [\"_time\", \"Daily Water Consumption (m³)\"])\n\ndp4 = from(bucket: \"AHK-AKDL\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\")\n  |> filter(fn: (r) => r[\"topic\"] == \"2024/BB/320\")\n  |> filter(fn: (r) => r[\"_field\"] == \"data_AN3\")\n  |> map(fn: (r) => ({\n       _time: r._time,\n       _value: ((float(v: r._value) - 4.0) / (20.0 - 4.0)) * 459.6/ 60.0 / 1000.0\n     }))\n  |> aggregateWindow(every: 1m, fn: sum)\n  |> sum(column: \"_value\")\n  |> map(fn: (r) => ({ _time: r._time, \"Daily Water Consumption (m³)\": r._value }))\n  |> keep(columns: [\"_time\", \"Daily Water Consumption (m³)\"])\n\n\nunion(tables: [dp2,dp3,dp4])\n  |> sum(column: \"Daily Water Consumption (m³)\")",
          "refId": "A"
        }
      ],
